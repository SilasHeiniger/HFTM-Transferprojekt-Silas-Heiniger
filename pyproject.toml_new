[project]
name = "password-manager"
version = "0.1.0"
description = "Simple password manager application"
requires-python = ">=3.8"

dependencies = [
    "flask>=3.0.0",
    "flask-sqlalchemy>=3.1.1",
    "psycopg2-binary>=2.9.9",
    "sqlalchemy>=2.0.23",
    "python-dotenv>=1.0.0",
]

[build-system]
requires = ["hatchling"]
build-backend = "hatchling.build"